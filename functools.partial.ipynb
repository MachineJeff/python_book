    "# 使用方法\n",
    "# functools.partial(func[,*args][, **kwargs])\n",
    "# 对现有函数进行参数固定，返回一个新的函数\n",
    "double = partial(multiply, y=2)\n",
    "\n",
    "print(double(3))\n",
    "print(double(4))\n",
    "print(double(5))"
   ]
